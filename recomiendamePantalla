    private void volverBotonActionPerformed(java.awt.event.ActionEvent evt) {                                            

        if(contPantalla == 0){
            deseleccionar();
            dispose();
        }else if (contPantalla >= 1){
            deseleccionar();
            volverMenuRecomendacion();
            contPantalla = 0;
        }

    }                                           

    private void literaturaCheckBoxActionPerformed(java.awt.event.ActionEvent evt) {                                                   

        if (literaturaCheckBox.isSelected()) {
            cienciaCheckBox.setSelected(false);
            arteCheckBox.setSelected(false);
            musicaCheckBox.setSelected(false);
        }
    }                                                  

    private void cienciaCheckBoxActionPerformed(java.awt.event.ActionEvent evt) {                                                

        if (cienciaCheckBox.isSelected()) {
            arteCheckBox.setSelected(false);
            literaturaCheckBox.setSelected(false);
            musicaCheckBox.setSelected(false);
        }

    }                                               

