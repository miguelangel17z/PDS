public void cambiarColorBoton(){
    
            avanzarBoton.setBackground(new Color(0,4,255));
            avanzarBoton.setText("ESCOGER");
        
}

public void devolverColorBoton(){
    
            avanzarBoton.setBackground(new Color(24,0,55));
            avanzarBoton.setText("AVANZAR");
        
}

    /**
     *
     */
public static void cerrarRecomiendamePantalla(JFrame instancia) {
        instancia.dispose(); 
    }





