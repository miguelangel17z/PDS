private void mostrarCheckBoxesPintura() {
    cursoPinturaCheckBox.setVisible(true);
    grupoPinturaCheckBox.setVisible(true);
    talleresPinturaCheckBox.setVisible(true);
    lineaPinturaCheckBox.setVisible(true);
     cambiarColorBoton();
    contPantalla++;
}

